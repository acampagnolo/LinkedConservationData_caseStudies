      </element>
      <optional>
        <oneOrMore>
          <element name="materialSpecification">
            <choice>
              <value>dyed</value>
              <value>undyed</value>
              <value>natural</value>
              <value>thin</value>
              <value>thick</value>
              <value>toned</value>
