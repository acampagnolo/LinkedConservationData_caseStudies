        </oneOrMore>
      </optional>
      <optional>
        <element name="materialCommercialName">
          <choice>
            <value>RK17</value>
            <value>Lascaux?</value>
            <value>Hasegawa</value>
            <value>SC6000</value>
            <value>RTRC50?</value>
